{
  "manifest_version": 3,
  "name": "Quote Master",
  "version": "1.0.1",
  "description": "Extension: Override the default newtab page for easier access to pages",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/logo192.png",
    "48": "icons/logo192.png",
    "128": "icons/logo512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/logo192.png",
      "48": "icons/logo192.png",
      "128": "icons/logo512.png"
    }
  }
}
