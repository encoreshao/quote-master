{
  "manifest_version": 3,
  "short_name": "Quote Master",
  "name": "Quote Master: Customized Efficiency Tool",
  "version": "1.2.4",
  "description": "Task Management, Bookmarks, GitLab, Extensions and Downloads. Customized Links can be accessed from the Default Newtab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "author": {
    "name": "Encore Shao",
    "email": "encore.shao@gmail.com"
  },
  "homepage_url": "https://ranbot.online",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "contextMenus",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Quote Master: Customized Efficiency Tool",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; worker-src 'self';"
  }
}
